<launch>
    <rosparam file="$(find ${PROJECT_NAME})/config/${PROJECT_NAME}.yaml" command="load" ns="${NODE_NAME}"/>
    <node pkg="rosbag" type="play" name="bag" args="-l /home/walker-ubuntu/Walkerspace/point_cloud_bags/track_all_1.bag" output="log"></node>
    <node pkg="${PROJECT_NAME}" type="${NODE_NAME}" name="${NODE_NAME}" output="screen"/>
